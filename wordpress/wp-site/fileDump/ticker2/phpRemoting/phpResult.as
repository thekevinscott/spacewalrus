package phpRemoting {	/* By Josh Strike // josh@joshstrike.com // Use freely and enjoy! */	public dynamic class phpResult {		//takes the NetConnection result and passes it back as a phpResult object,		//w/ a breakdown of the constituent objects.		function phpResult(endRes:Object,toFunction:Function) {			this.toFunction = toFunction;			this.result = endRes.result;			this.log = endRes.log;			this.toFunction(this);		}	}}